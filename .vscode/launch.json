  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Debug File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Debug Faster-RCNN",
      "type": "debugpy",
      "request": "launch",
      "program": "./train/faster-rcnn.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Streamlit: Local App",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": ["run", "${workspaceFolder}/the_app/streamlit_app.py"],
      "justMyCode": true
    }
  ]
}
